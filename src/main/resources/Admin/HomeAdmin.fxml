<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane fx:id="anchorPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.project.app.Admin.HomeAdmin">
    <children>
        <ImageView fitHeight="600.0" fitWidth="800.0">
            <image>
                <Image url="@../image/blue.png" />
            </image>
        </ImageView>
        <TableView fx:id="table" layoutY="26.0" prefHeight="575.0" prefWidth="800.0">
            <columns>
                <TableColumn fx:id="idtab1" prefWidth="34.0" text="ID" />
                <TableColumn fx:id="name" minWidth="0.0" prefWidth="217.0" text="Username" />
                <TableColumn fx:id="email" prefWidth="176.0" text="Email" />
                <TableColumn fx:id="phone" prefWidth="181.0" text="Mobile Phone" />
                <TableColumn fx:id="pers" prefWidth="111.0" text="Account" />
                <TableColumn fx:id="action" text="Action" />
            </columns>
        </TableView>
        <Pane layoutX="-1.0" prefHeight="26.0" prefWidth="800.0" style="-fx-background-color: #2D3447;">
            <children>
                <ImageView fitHeight="20.0" fitWidth="20.0" layoutX="750.0" layoutY="3.0" onMouseClicked="#minimize">
                    <image>
                        <Image url="@../image/circl.png" />
                    </image>
                </ImageView>
                <ImageView fx:id="minimizeCloseIcon" fitHeight="20.0" fitWidth="20.0" layoutX="777.0" layoutY="3.0" onMouseClicked="#close">
                    <image>
                        <Image url="@../image/interfac.png" />
                    </image>
                </ImageView>
                <ImageView fx:id="id_exit" fitHeight="20.0" fitWidth="20.0" layoutX="3.0" layoutY="3.0" onMouseClicked="#exit">
                    <image>
                        <Image url="@../image/return.png" />
                    </image>
                </ImageView>
            </children>
        </Pane>
    </children>
    <stylesheets>
        <URL value="@../style.css" />
    </stylesheets>
</AnchorPane>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="view_page" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.project.app.NonLoggedUser.ViewFlightsController">
   <children>
      <TableView fx:id="table" prefHeight="561.0" prefWidth="800.0">
        <columns>
          <TableColumn fx:id="id_table" prefWidth="38.0" text="ID" />
          <TableColumn fx:id="location_table" minWidth="0.0" prefWidth="183.0" text="Location" />
            <TableColumn fx:id="destination_table" prefWidth="146.0" text="Destination" />
            <TableColumn fx:id="date_table" prefWidth="120.0" text="Date" />
            <TableColumn fx:id="price_table" prefWidth="110.0" text="Price" />
            <TableColumn fx:id="hour_table" prefWidth="77.0" text="Hour" />
            <TableColumn fx:id="seats_table" prefWidth="125.0" text="Seats" />
        </columns>
      </TableView>
      <TextField fx:id="date_field" layoutX="646.0" layoutY="569.0" promptText="Data [ yyyy.mm.dd ]" />
      <TextField fx:id="destination_field" layoutX="490.0" layoutY="569.0" promptText="Destinatia" />
      <TextField fx:id="location_field" layoutX="335.0" layoutY="569.0" promptText="Locatie" />
      <Button fx:id="id_cauta" layoutX="244.0" layoutY="569.0" mnemonicParsing="false" onMouseClicked="#search_flight" prefHeight="25.0" prefWidth="84.0" text="Cauta" />
      <Button fx:id="id_back" layoutX="14.0" layoutY="567.0" mnemonicParsing="false" onMouseClicked="#back" prefHeight="25.0" prefWidth="84.0" text="BACK">
         <font>
            <Font name="System Bold" size="13.0" />
         </font></Button>
      <Button layoutX="163.0" layoutY="569.0" mnemonicParsing="false" onMouseClicked="#refresh_button" prefHeight="25.0" prefWidth="72.0" text="Refresh" />
   </children>
</AnchorPane>

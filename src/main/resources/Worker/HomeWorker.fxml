            <Button fx:id="id_add" layoutX="41.0" layoutY="230.0" onMouseClicked="#add_flight" prefHeight="25.0" prefWidth="143.0" style="-fx-background-color: #7eb5ec; -fx-background-radius: 20;" text="Add flight">
               <font>
                  <Font size="14.0" />
               </font>

<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.project.app.Worker.HomeWorkerController">
   <children>
      <Pane prefHeight="600.0" prefWidth="230.0" style="-fx-background-color: #2D3447;">
         <children>
            <ImageView fitHeight="600.0" fitWidth="255.0" layoutY="1.0">
               <image>
                  <Image url="@../image/blue-gradient.png" />
               </image>
            </ImageView>
            <ImageView fitHeight="130.0" fitWidth="130.0" layoutX="58.0" layoutY="34.0">
               <image>
                  <Image url="@../image/aviaTrans.png" />
               </image>
            </ImageView>
            <Line endX="41.5" layoutX="153.0" layoutY="175.0" startX="-100.0" stroke="WHITE" strokeWidth="2.0" />
            <Label fx:id="id_lab" layoutX="42.0" layoutY="195.0" prefHeight="31.0" prefWidth="171.0" text="Personal cabinet" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="21.0" />
               </font>
            </Label>
            <Line endX="41.5" layoutX="153.0" layoutY="509.0" startX="-100.0" stroke="WHITE" strokeWidth="2.0" />
            <Button fx:id="id_news" layoutX="52.0" layoutY="429.0" onMouseClicked="#news" prefHeight="25.0" prefWidth="143.0" text="News">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="id_tick" layoutX="52.0" layoutY="374.0" onMouseClicked="#buy_ticket" prefHeight="25.0" prefWidth="143.0" text="Tickets purchased">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button layoutX="52.0" layoutY="319.0" onMouseClicked="#view_flights" prefHeight="25.0" prefWidth="143.0" text="View all flight">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="id_add" layoutX="52.0" layoutY="262.0" onMouseClicked="#add_flight" prefHeight="25.0" prefWidth="143.0" text="Add flight">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>

         </children>
      </Pane>
      <Pane prefHeight="26.0" prefWidth="800.0" style="-fx-background-color: #2D3447; -fx-background-radius: 0 0 0  13;">
         <children>
            <ImageView fitHeight="20.0" fitWidth="20.0" layoutX="750.0" layoutY="3.0" onMouseClicked="#minimize">
               <image>
                  <Image url="@../image/circl.png" />
               </image>
            </ImageView>
            <ImageView fx:id="minimizeCloseIcon" fitHeight="20.0" fitWidth="20.0" layoutX="777.0" layoutY="3.0" onMouseClicked="#close">
               <image>
                  <Image url="@../image/interfac.png" />
               </image>
            </ImageView>
            <ImageView fx:id="exit" fitHeight="20.0" fitWidth="20.0" layoutX="3.0" layoutY="3.0" onMouseClicked="#exitIcon">
               <image>
                  <Image url="@../image/return.png" />
               </image>
            </ImageView>
         </children>
      </Pane>
      <AnchorPane fx:id="secondPage" layoutX="250.0" layoutY="26.0" prefHeight="575.0" prefWidth="550.0">
         <children>
            <ImageView fitHeight="575.0" fitWidth="550.0">
               <image>
                  <Image url="@../image/workerHome.png" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
   </children>
   <stylesheets>
      <URL value="@../style.css" />
   </stylesheets>
</AnchorPane>
